    <decisions>
        <name>Are_student_counts_Low_Enough</name>
        <label>Are student counts Low Enough</label>
        <locationX>446</locationX>
        <locationY>398</locationY>
        <defaultConnector>
            <targetReference>Get_Internal_User</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Go Internal</defaultConnectorLabel>
        <rules>
            <name>Go_External</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>varStudentCount</leftValueReference>
                <operator>LessThan</operator>
                <rightValue>
                    <numberValue>200.0</numberValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>varInstitutionStudentCount</leftValueReference>
                <operator>LessThan</operator>
                <rightValue>
                    <numberValue>5000.0</numberValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Get_External_User</targetReference>
            </connector>
            <label>Go External</label>
        </rules>
    </decisions>
    <decisions>
        <name>Check_School_Type</name>
        <label>Check School Type</label>
        <locationX>358</locationX>
        <locationY>158</locationY>
        <defaultConnector>
            <targetReference>Get_Default_User</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Wrong School Type</defaultConnectorLabel>
        <rules>
            <name>Non_Faculty</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>varFacStatus</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>confirmed_faculty</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>varLeadSource</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Account Creation</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Get_Default_User</targetReference>
            </connector>
            <label>Non-Faculty</label>
        </rules>
        <rules>
            <name>X2_4_year</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>varSchoolType</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>College/University (4)</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>varSchoolType</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Technical/Community College (2)</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Is_School_KIP</targetReference>
            </connector>
            <label>2/4 year</label>
        </rules>
    </decisions>
    <decisions>
        <name>Is_School_KIP</name>
