    <decisions>
        <name>Does_opp_need_to_be_renewed</name>
        <label>Does opp need to be renewed</label>
        <locationX>417</locationX>
        <locationY>255</locationY>
        <defaultConnector>
            <targetReference>Update_Opp2</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Nope - End</defaultConnectorLabel>
        <rules>
            <name>Department</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>varOpp.Department_Adoption__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_Opp2</targetReference>
            </connector>
            <label>Department</label>
        </rules>
        <rules>
            <name>Yes_needs_renewed</name>
            <conditionLogic>1 AND (2 OR 3)</conditionLogic>
            <conditions>
                <leftValueReference>varOpp.Renewal_Status__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Needs Renewal</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>varOpp.Last_In_Touch_Date__c</leftValueReference>
                <operator>GreaterThan</operator>
                <rightValue>
                    <dateValue>2018-06-30</dateValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>varOpp.Recent_Resource_Date__c</leftValueReference>
                <operator>GreaterThan</operator>
                <rightValue>
                    <dateValue>2018-06-30</dateValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_Opp</targetReference>
            </connector>
            <label>Yes needs renewed</label>
        </rules>
    </decisions>
