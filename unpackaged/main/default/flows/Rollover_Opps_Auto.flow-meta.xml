    <description>recent resource date</description>
    <formulas>
        <name>fCurrentDate</name>
        <dataType>String</dataType>
        <expression>TEXT(YEAR({!$Flow.CurrentDate}))
&amp;&quot;-&quot;&amp;
IF(LEN(TEXT(MONTH({!$Flow.CurrentDate})))&lt;2,&quot;0&quot;,&quot;&quot;)&amp;TEXT(MONTH({!$Flow.CurrentDate}))
&amp;&quot;-&quot;&amp;
IF(LEN(TEXT(DAY({!$Flow.CurrentDate})))&lt;2,&quot;0&quot;,&quot;&quot;)&amp;TEXT(DAY({!$Flow.CurrentDate}))</expression>
    </formulas>
    <formulas>
        <name>fDescription</name>
        <dataType>String</dataType>
        <expression>{!fCurrentDate}&amp;&quot;, &quot;&amp;{!$User.FirstName}&amp;{!$User.LastName}&amp;&quot;, &quot;&amp;&quot; Rollover &quot;&amp;SUBSTITUTE({!$Label.NewLine},&quot;-&quot;,&quot;&quot;)&amp;{!varOpp.Description}</expression>
    </formulas>
    <formulas>
        <name>fDupCheck</name>
        <dataType>String</dataType>
        <expression>LEFT({!varOpp.sort__c}, LEN({!varOpp.sort__c})-4)&amp;TEXT({!varOpp.Base_Year__c}+1)</expression>
    </formulas>
    <formulas>
        <name>fDupType</name>
        <dataType>String</dataType>
        <expression>IF( {!varDupType} = &quot;Renewal - Verified&quot; , {!varDupType} , &quot;Renewal&quot;)</expression>
    </formulas>
    <formulas>
        <name>fFutureBaseYear</name>
        <dataType>Number</dataType>
        <expression>{!varOpp.Base_Year__c}+1</expression>
        <scale>0</scale>
    </formulas>
    <interviewLabel>Rollover Opps {!$Flow.CurrentDateTime}</interviewLabel>
    <label>Rollover Opps Auto</label>
