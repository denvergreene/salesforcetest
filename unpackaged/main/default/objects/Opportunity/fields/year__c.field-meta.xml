<?xml version="1.0" encoding="utf-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>year__c</fullName>
    <description>This field is used for catching duplicates. Because the Opportunity is recreated every year it needs to account for that, and this is how.</description>
    <externalId>false</externalId>
    <formula>TEXT(
IF(Month(Class_Start_Date__c )&lt;7, 
Year(Class_Start_Date__c)-1, 
Year( Class_Start_Date__c )) 
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>year</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
