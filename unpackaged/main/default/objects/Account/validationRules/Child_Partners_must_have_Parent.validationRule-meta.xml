<?xml version="1.0" encoding="utf-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Child_Partners_must_have_Parent</fullName>
    <active>true</active>
    <errorConditionFormula>AND( ISPICKVAL(Type, "Child") ,  ISBLANK( ParentId ) )</errorConditionFormula>
    <errorMessage>If child, then you have to assign a parent.</errorMessage>
</ValidationRule>
