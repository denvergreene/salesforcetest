<?xml version="1.0" encoding="utf-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>TermYear__c</fullName>
    <externalId>false</externalId>
    <formula>CASE(IF(Month(Class_Start_Date__c) &gt;6, 
year( Class_Start_Date__c ), 
year( Class_Start_Date__c )-1), 
2011, "2011 - 12", 
2012, "2012 - 13", 
2013, "2013 - 14", 
2014, "2014 - 15", 
2015, "2015 - 16", 
2016, "2016 - 17", 
2017, "2017 - 18", 
2018, "2018 - 19", 
2019, "2019 - 20", 
2020, "2020 - 21", 
2021, "2021 - 22", 
2022, "2022 - 23", 
"fix formula") &amp;" "&amp; IF(MONTH(Class_Start_Date__c)&gt;6, "Fall", "Spring")</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>TermYear</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
